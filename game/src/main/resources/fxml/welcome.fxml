<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="659.0" prefWidth="1129.0" stylesheets="@../css_files/button.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.welcome_controller">

    <children>
        <ImageView fitHeight="677.0" fitWidth="1128.0" pickOnBounds="true">
            <image>
                <Image url="@../BACKGROUND.jpeg" />
            </image>
        </ImageView>
        <ImageView accessibleRole="BUTTON" fitHeight="89.0" fitWidth="241.0" layoutX="448.0" layoutY="149.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../NEWGAME.png" />
            </image>
         <effect>
            <Glow level="1.0" />
         </effect>
        </ImageView>
        <ImageView accessibleRole="BUTTON" fitHeight="94.0" fitWidth="241.0" layoutX="450.0" layoutY="246.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../LOAD.png" />
            </image>
         <effect>
            <Glow level="1.0" />
         </effect>
        </ImageView>
        <ImageView accessibleRole="BUTTON" fitHeight="72.0" fitWidth="241.0" layoutX="450.0" layoutY="341.0" pickOnBounds="true">
            <image>
                <Image url="@../EXIT.png" />
            </image>
         <effect>
            <Glow level="1.0" />
         </effect>
        </ImageView>
      <Button fx:id="new_game_button" layoutX="454.0" layoutY="148.0" mnemonicParsing="false" onAction="#new_game_clicked" prefHeight="72.0" prefWidth="231.0" />
      <Button fx:id="load_button" layoutX="455.0" layoutY="246.0" mnemonicParsing="false" onAction="#load_game_clicked" prefHeight="72.0" prefWidth="231.0" />
      <Button fx:id="exit_button" layoutX="456.0" layoutY="340.0" mnemonicParsing="false" onAction="#exit_clicked" prefHeight="65.0" prefWidth="231.0" />
    </children>

</AnchorPane>
